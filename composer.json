{
    "name": "roslov/queue-bundle",
    "type": "symfony-bundle",
    "description": "Queue bundle",
    "keywords": [
        "RabbitMQ",
        "queue",
        "bundle"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-sockets": "*",
        "doctrine/orm": "^2.11|^3.0",
        "php-amqplib/rabbitmq-bundle": "^2.12",
        "roslov/log-obfuscator": "^1.0",
        "symfony/config": "^6.0|^7.0",
        "symfony/dependency-injection": "^6.0|^7.0",
        "symfony/framework-bundle": "^6.0|^7.0",
        "symfony/property-access": "^6.0|^7.0",
        "symfony/serializer": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roslov/psr12ext": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Roslov\\QueueBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Roslov\\QueueBundle\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true,
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "support": {
        "source": "https://github.com/roslov/queue-bundle"
    }
}
